{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "quotes": [2, "single"],
        "no-underscore-dangle": 0,
        "curly": 0,
        "consistent-return": 0,
        "new-cap": 0,
        "strict": [2, "never"],
        "indent": [2, 4, { "SwitchCase": 1 }],
        "no-shadow": 0,
        "semi-spacing": 0,
        "semi": [2, "always"]

    },
    "globals": {
        "L": true,
        "kosmtik": true
    }
}
